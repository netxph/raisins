@{
    ViewBag.Title = "Dashboard";
}
<br /><br /><br /><br /><br />
<div id="groupvote">
<table id="votes-grid" >
    <caption>GROUP VOTES SUMMARY</caption>
   
    <thead>
        <tr >
          <th>GROUP</th>
         
            <th scope="col">Locked</th>
            <th scope="col">Total</th>
            <th scope="col">Locked Pay</th>
            <th scope="col">Total Pay</th>
            
        </tr>
    </thead>
    <tbody>

        @foreach (var team in ViewBag.Votes)
        { 
        <tr>
            <th  id="teamnames" scope="row">@team.Name</th>
            <td>@team.LockedVotes</td>
            <td>@team.TotalVotes</td>
            <td>@team.LockedPayments.ToString("0.00")</td>
            <td>@team.TotalPayments.ToString("0.00")</td>
        </tr>
        }
    </tbody>
</table>

</div>

<br /><br /><br />

 <caption>OVERALL VOTES SUMMARY</caption>
<table id="votessum">
    <thead>   
        <tr>
            <th scope="col">Locked</th>
            <th scope="col">Total</th>
            <th scope="col">Locked Pay</th>
            <th scope="col">Total Pay</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@ViewBag.Overall.LockedVotes</td>
            <td>@ViewBag.Overall.TotalVotes</td>
            <td>@ViewBag.Overall.LockedPayments.ToString("0.00")</td>
            <td>@ViewBag.Overall.TotalPayments.ToString("0.00")</td>
        </tr>
    </tbody>
</table>

